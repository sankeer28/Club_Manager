{
    "builds": [
        {
            "src": "Flask/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "2048mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "Flask/static/$1"
        },
        {
            "src": "/templates/(.*)",
            "dest": "Flask/templates/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Flask/main.py"
        }
    ]
}
